{
    "format_version": "1.10.0",
    "animation_controllers": {
		"controller.animation.loitering_munition_launcher.general": {
			"initial_state" : "idle",
			"states" : {
				"equip": {
					"animations": [
						{
							"loitering_munition_launcher_1st_equip": "c.is_first_person??v.is_first_person"
						},
						{
							"loitering_munition_launcher_3rd_equip": "!(c.is_first_person??v.is_first_person)"
						}
					],
					"transitions" : [
						{
							"idle" : "q.any_animation_finished"
						}
					]
				},
				"idle": {
					"animations": [
						{
							"loitering_munition_launcher_1st_idle": "c.is_first_person??v.is_first_person"
						},
						{
							"loitering_munition_launcher_3rd_idle": "!(c.is_first_person??v.is_first_person)"
						}
					],
					"transitions" : [
						{
							"transition": "q.mod.loitering_munition_transition"
						},
						{
							"equip": "q.mod.loitering_munition_equip"
						}
					]
				},
				"transition": {
					"animations": [
						{
							"loitering_munition_launcher_1st_transition": "c.is_first_person??v.is_first_person"
						},
						{
							"loitering_munition_launcher_3rd_transition": "!(c.is_first_person??v.is_first_person)"
						}
					],
					"transitions" : [
						{
							"idle" : "!q.mod.loitering_munition_transition && q.mod.loitering_munition_idle"
						},
						{
							"run" : "!q.mod.loitering_munition_transition && q.mod.loitering_munition_run"
						},
						{
							"aim" : "!q.mod.loitering_munition_transition && q.mod.loitering_munition_aim"
						},
						{
							"inspect" : "!q.mod.loitering_munition_transition && q.mod.loitering_munition_inspect"
						},
						{
							"transition2" : "q.mod.loitering_munition_re_transition"
						}
					]
				},
				"transition2": {
					"animations": [
						{
							"loitering_munition_launcher_1st_transition": "c.is_first_person??v.is_first_person"
						},
						{
							"loitering_munition_launcher_3rd_transition": "!(c.is_first_person??v.is_first_person)"
						}
					],
					"transitions" : [
						{
							"idle" : "!q.mod.loitering_munition_transition && q.mod.loitering_munition_idle"
						},
						{
							"run" : "!q.mod.loitering_munition_transition && q.mod.loitering_munition_run"
						},
						{
							"aim" : "!q.mod.loitering_munition_transition && q.mod.loitering_munition_aim"
						},
						{
							"inspect" : "!q.mod.loitering_munition_transition && q.mod.loitering_munition_inspect"
						},
						{
							"transition" : "q.mod.loitering_munition_re_transition"
						}
					]
				},
				"run": {
					"animations": [
						{
							"loitering_munition_launcher_1st_run": "c.is_first_person??v.is_first_person"
						},
						{
							"loitering_munition_launcher_3rd_run": "!(c.is_first_person??v.is_first_person)"
						}
					],
					"transitions" : [
						{
							"transition": "q.mod.loitering_munition_transition"
						},
						{
							"equip": "q.mod.loitering_munition_equip"
						}
					]
				},
				"aim": {
					"animations": [
						{
							"loitering_munition_launcher_1st_aim": "c.is_first_person??v.is_first_person"
						},
						{
							"loitering_munition_launcher_3rd_aim": "!(c.is_first_person??v.is_first_person)"
						}
					],
					"transitions" : [
						{
							"transition": "q.mod.loitering_munition_transition"
						},
						{
							"equip": "q.mod.loitering_munition_equip"
						}
					]
				},
				"inspect": {
					"animations": [
						{
							"loitering_munition_launcher_1st_inspect": "c.is_first_person??v.is_first_person"
						},
						{
							"loitering_munition_launcher_3rd_inspect": "!(c.is_first_person??v.is_first_person)"
						}
					],
					"transitions" : [
						{
							"transition": "q.mod.loitering_munition_transition"
						},
						{
							"equip": "q.mod.loitering_munition_equip"
						}
					]
				}
			}
		}
	}
}