{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "animations": {
        "move": "animation.loitering_munition.move"
      },
      "geometry": {
        "default": "geometry.loitering_munition"
      },
      "identifier": "orchiella:loitering_munition",
      "materials": {
        "default": "netease_entity_metallic",
        "unlit": "entity_emissive_alpha"
      },
      "render_controllers": [
        "controller.render.loitering_munition"
      ],
      "scripts": {
        "pre_animation": [
          "variable.shake = query.shake_time - query.frame_alpha;",
          "variable.shake_power = variable.shake > 0.0 ? -Math.sin(variable.shake * 200.0) * variable.shake : 0.0;"
        ],
        "animate": [
          "move"
        ]
      },
      "textures": {
        "default": "textures/entity/loitering_munition_launcher"
      }
    }
  }
}